    "sandbox": "sudo node -r dotenv/config dist/sandbox/main.js",
    "start_sandbox_nodemon": "sudo node_modules/nodemon/bin/nodemon.js -r dotenv/config dist/sandbox/main.js",
    "build_ts": "tsc --jsx react-jsx",
    "rebuild_ts": "tsc --build --force",
    "build_and_watch_ts1": "rm -rf ./dist/ && tsc --watch --jsx react-jsx",
    "build_and_watch_ts": "rm -rf ./dist/ && mkdir -p ./dist/sandbox && touch ./dist/sandbox/run.js && chmod +x ./dist/sandbox/run.js && tsc --watch",
    "build_babel": "babel client -d client_lib",
    "build_and_watch_babel": "babel client -d client_lib --watch",
    "build_webpack": "webpack --config webpack.config.cjs",
    "build_and_watch_webpack": "webpack --config webpack.config.cjs --watch",
    "test_raw": "sudo node -r dotenv/config _tests/main.js",
    "test": "jest --config=dist/jest.config.js"
